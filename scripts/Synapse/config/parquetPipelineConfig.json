{
    "name": "Parquet Pipeline",
    "properties": {
        "activities": [
            {
                "name": "Convert Parquet MPI",
                "type": "SynapseNotebook",
                "dependsOn": [],
                "policy": {
                    "timeout": "0.12:00:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                },
                "userProperties": [],
                "typeProperties": {
                    "notebook": {
                        "referenceName": "Convert Parquet MPI",
                        "type": "NotebookReference"
                    },
                    "snapshot": true,
                    "sparkPool": {
                        "referenceName": "sparkpool",
                        "type": "BigDataPoolReference"
                    },
                    "executorSize": "Medium",
                    "conf": {
                        "spark.dynamicAllocation.enabled": false
                    },
                    "driverSize": "Medium"
                }
            }
        ],
        "annotations": []
    }
}