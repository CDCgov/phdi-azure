{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "blob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "source-data/{folder}/{name}.{extension}",
      "connection": "PhiStorage"
    },
    {
      "type": "serviceBus",
      "direction": "out",
      "connection": "AzureServiceBusConnectionString",
      "name": "queue",
      "queueName": "%ServiceBusQueueName%"
    }
  ]
}